<?xml version="1.0"?>
<launch>
  <!-- Load joint controller configurations from YAML file to parameter server -->
  <group ns="me6_robot">
    <rosparam file="$(find me6_moveit)/config/ros_controllers.yaml" command="load"/>

    <!-- Load the controllers under /me6_robot -->
    <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false"
          output="screen" args="joint_state_controller joint_controller"/>
  </group>
</launch>
